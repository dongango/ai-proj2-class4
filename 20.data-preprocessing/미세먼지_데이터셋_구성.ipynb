   "source": [
    "from pandas.tseries.offsets import MonthEnd\n",
    "\n",
    "summary['start_date'] = pd.to_datetime(summary['datetime'])  # 1일\n",
    "summary['end_date'] = summary['start_date'] + MonthEnd(0)   # 말일\n",
    "\n",
    "# 일자 리스트 생성\n",
    "date_rows = []\n",
    "\n",
    "for _, row in summary.iterrows():\n",
    "    days = pd.date_range(start=row['start_date'], end=row['end_date'], freq='D')\n",
    "    for day in days:\n",
    "        date_rows.append({\n",
    "            'date': day,\n",
    "            'region': row['region'],\n",
    "            'fuel_type': row['fuel_type'],\n",
    "            'reg_count': row['reg_count'],\n",
    "        })\n",
    "\n",
    "daily_df = pd.DataFrame(date_rows)"
   ]
